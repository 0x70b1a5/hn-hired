{
  "extends": [
    "react",
    "prettier"
  ],
  "plugins": [
    "react",
    "prettier",
    "standard"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "react/react-in-jsx-scope": "off"
  }
}
